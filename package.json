    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "google-auth-library": "^7.12.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.2",
    "nodemailer-mailgun-transport": "^2.1.3",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
